<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.flyexp.douban_movie">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MovieThemeTransNav">
        <activity
            android:name=".view.activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.SearchDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.MovieDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.PhotoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.CelebrityDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.FavoriteActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.GiftActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>